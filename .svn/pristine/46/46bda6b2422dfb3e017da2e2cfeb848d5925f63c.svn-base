package com.oa.controller.mobile;

import java.util.HashMap;
import java.util.Map;

import com.zhilian.model.T_User;

public class BussinessContent {

    private static Map<String, T_User> userMap = new HashMap<String, T_User>();
    private static int itemid;
    
    public void setUserMap(Map<String, T_User> userMap){
	BussinessContent.userMap=userMap;
    }
    
    public Map<String, T_User> getUserMap(){
        return userMap;
    }
    
    public void setItemid(int itemid){
	BussinessContent.itemid=itemid;
    }
    
    public int getItemid(){
	return itemid;
    }
}
