<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="/include/inc.jsp"%>
<div class="bjui-pageContent">
        <table class="wordInfo" align="center" style="width: 98%;">
            <tr>
                <td class="title">领　　导</td>
                <td class="normal">
                    ${leaderName}
                </td>
            </tr>
            <tr>
                <td width="25%" class="title">开始时间</td>
                <td width="75%" class="normal">
                    ${leader_Schedule.date}日${leader_Schedule.hour}时${leader_Schedule.minute}分
                </td>
            </tr>
            <tr>
                <td width="25%" class="title">结束时间</td>
                <td width="75%" class="normal">
                    ${leader_Schedule.edate}日${leader_Schedule.ehour}时${leader_Schedule.eminute}分
                </td>
            </tr>
            <tr>
                <td class="title">日程标题</td>
                <td class="normal">${leader_Schedule.title} </td>
            </tr>
            <tr>
                <td class="title">日程内容</td>
                <td><pre style="min-height:150px;background-color:transparent;border-color:transparent;line-height: 20px;">${leader_Schedule.content}</pre></td>
            </tr>
        </table>
</div>
<div class="bjui-pageFooter">
	<ul>
		<li><button type="button" class="btn-close" data-icon="close">关闭</button></li>
	</ul>
</div>
